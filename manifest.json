{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0.0",
  "description": "A brief description of my extension.",
  "icons": [
    {
      "src": "icon.png",
      "sizes": "48x48",
      "type": "image/png"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "image.jfif",
      "32": "image.jfif",
      "48": "image.jfif",
      "128": "image.jfif"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "popup.js"
      ]
    }
  ]
}
